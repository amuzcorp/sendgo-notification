{
    "name": "techigh/sendgo-notification",
    "description": "Notification Package for Laravel",
    "license": "MIT",
    "keywords": [
        "notification",
        "message"
    ],
    "autoload": {
        "psr-4": {
            "Techigh\\SendgoNotification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Techigh\\SendgoNotification\\SendgoNotificationServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Techigh",
            "email": "techigh@amuz.co.kr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0|^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/notifications": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "config": {
        "sort-packages": true
    }
}
